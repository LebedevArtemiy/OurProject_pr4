#ifndef LIBRARY_H
#define LIBRARY_H

#include <QString>

class Treygol
{
public:

    Treygol();
    void set(const double &a, const double &b, const double &c);
    double getP() const;
    double getS() const;
    double getK() const;
    double getK1() const;
    double getK2() const;
    double getK3() const;

private:
    double a,b,c, S, P, k1, k2, k3;

};

#endif // LIBRARY_H
